	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"id" : "01C0H0Z5Z8MCGG0J0N3YAACP3Y",
			"allow-contrib" : false
		}
	}
