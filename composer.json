{
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": "^7.0.8",
		"doctrine/common": "^2.8",
		"doctrine/dbal": "^2.6",
		"doctrine/orm": "^2.5",
		"symfony/console": "^3.4",
		"symfony/flex": "^1.0",
		"symfony/framework-bundle": "^3.4",
		"symfony/lts": "^3",
		"symfony/maker-bundle": "^1.0",
		"symfony/monolog-bundle": "^3.1",
		"symfony/orm-pack": "^1.0",
		"symfony/serializer-pack": "^1.0",
		"symfony/yaml": "^3.4"
	},
	"require-dev" : {
		"symfony/dotenv" : "^3.4"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
